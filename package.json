  "dependencies": {
    "browser-sync": "~2.8.2",
    "browserify": "~11.0.1",
    "bundle-collapser": "~1.2.1",
    "del": "~2.0.2",
    "extend": "~3.0.0",
    "gulp": "~3.9.0",
