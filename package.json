{
  "name": "gulp-appfy-tasks",
  "version": "3.2.2",
  "description": "Set of gulp tasks used in slush-seed-appfy",
  "main": "index.js",
  "dependencies": {
    "browser-sync": "^2.11.2",
    "browserify": "^13.0.0",
    "bundle-collapser": "^1.2.1",
    "del": "^2.2.0",
    "exorcist": "^0.4.0",
    "extend": "^3.0.0",
    "gulp": "^3.9.1",
    "gulp-notify": "^2.2.0",
    "gulp-plumber": "^1.1.0",
    "gulp-postcss": "^6.1.0",
    "gulp-sourcemaps": "^1.6.0",
    "gulp-uglify": "^1.5.3",
    "gulp-util": "^3.0.7",
    "gulp-watch": "^4.3.5",
    "postcss": "^5.0.19",
    "postcss-copy": "^3.1.0",
    "postcss-discard-comments": "^2.0.4",
    "postcss-import": "^8.1.0",
    "run-sequence": "^1.1.5",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.1.0",
    "vinyl-transform": "^1.0.0",
    "watchify": "^3.7.0"
  },
  "devDependencies": {
    "babel-cli": "^6.5.1",
    "babel-plugin-add-module-exports": "^0.1.2",
    "babel-plugin-transform-object-assign": "^6.5.0",
    "babel-preset-es2015": "^6.5.0",
    "conventional-changelog-cli": "^1.1.1",
    "del-cli": "^0.2.0",
    "eslint": "^2.7.0",
    "eslint-config-postcss": "^2.0.2",
    "eslint-config-tinchoz49": "^2.1.0"
  },
  "files": [
    "dist",
    "default-config.json"
  ],
  "scripts": {
    "test": "eslint src",
    "clean": "del-cli dist",
    "build": "npm run clean && babel src --source-maps  --out-dir dist",
    "start": "npm run clean && babel src --watch --source-maps --out-dir dist",
    "prepublish": "npm run build",
    "version": "conventional-changelog -i CHANGELOG.md --same-file && git add ."
  },
  "eslintConfig": {
    "extends": "tinchoz49"
  },
  "babel": {
    "presets": [
      "es2015"
    ],
    "plugins": [
      "transform-object-assign",
      "add-module-exports"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/geut/gulp-appfy-tasks.git"
  },
  "keywords": [
    "gulp",
    "tasks",
    "slush-seed-appfy",
    "seed",
    "browserify",
    "postcss"
  ],
  "author": "Geut <geut.studio@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/geut/gulp-appfy-tasks/issues"
  },
  "homepage": "https://github.com/geut/gulp-appfy-tasks#readme"
}
