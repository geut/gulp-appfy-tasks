{
  "sourcePath": "src",
  "destPath": "dist",
  "assetsTemplate": "assets/[hash].[ext]",
  "entryCss": "index.css",
  "entryJs": "index.js",
  "entryHtml": "index.html",
  "browsersync": {
    "port": 3000,
    "notify": false,
    "server": {
      "baseDir": "./"
    }
  },
